{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Error"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LocalDb": "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=TicketingDB;Trusted_Connection=True;",
    "TicketingDB": "#ConnectionStrings.TicketingDb#",
    "ServiceBus": "#ConnectionStrings.ServiceBus#"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "#ApplicationInsights.InstrumentationKey#"
  },
  "IdentityProvider": {
    "IdpAuthorityURL": "#IdentityProvider.IdpAuthorityURL#",
    "IdpApiName": "#IdentityProvider.IdpApiName#"
  },
  "Security": {
    "VaultName": "#Security.VaultName#"
  }
}